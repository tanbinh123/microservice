<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.javaweb.web.dao.ds1.UserModuleDao">

	<select id="getModuleIdsByUserId" parameterType="java.lang.String" resultType="java.lang.String">
		select module_id from sys_user_module where user_id = #{_parameter}
	</select>
	
</mapper>